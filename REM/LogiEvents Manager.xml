 oid = "UC-0001">
<rem:name>
Agregar Evento</rem:name>
<rem:version>
<rem:major>1</rem:major>
<rem:minor>0</rem:minor>
<rem:date>
<rem:year>2025</rem:year>
<rem:month>3</rem:month>
<rem:day>24</rem:day>
